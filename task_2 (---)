N = int(input())
A = []
k = 0
p = 0
u = 0
i = 0
j = 0
for i in range(0, N):
    A.append(int(input()))

i = 0

while i < len(A):
    while j < len(A):
        if A[i] == A[j] and i != j:
            p = i
            u = j
            print(p, u)
        j += 1
        print(j)
    i += 1  
    print(i)

print("____________________________")
print(len(A))
print(A)
