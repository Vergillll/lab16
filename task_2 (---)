N = int(input())
A = []
rep = []
p = 0
u = 0


k = 0
i = 0
j = 0
for i in range(0, N):
    A.append(int(input()))

i = 0

while i < len(A):
    j = 0
    while j < len(A):
        if A[i] == A[j] and i != j and j > i:
            p = i
            u = j
            print(p, u)
        j += 1
    i += 1

print("____________________________")
print(len(A))
print(A)
